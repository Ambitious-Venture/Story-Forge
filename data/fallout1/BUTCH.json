      "target": "ButchEndTransport"
    },
    {
      "text": "",
      "source": "talk_p_proc_1",
      "target": "Butch37"
    },
    {
      "text": "",
      "source": "talk_p_proc_1",
      "target": "Butch60"
    },
    {
      "text": "",
      "source": "talk_p_proc_1",
      "target": "get_reaction"
    },
    {
      "text": "",
      "source": "talk_p_proc_1",
      "target": "Butch73"
    },
    {
      "text": "",
      "source": "talk_p_proc_1",
      "target": "Butch59"
    },
    {
      "text": "",
      "source": "talk_p_proc_1",
      "target": "Butch01"
    },
    {
      "text": "Okay, thanks.",
      "source": "Butch05",
      "target": "ButchEndAccept"
    },
    {
      "text": "One more question, okay?",
      "source": "Butch05",
      "target": "Butch06_1"
    }
  ]
