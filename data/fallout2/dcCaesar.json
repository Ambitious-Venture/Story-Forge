    {
      "text": "",
      "node_id": "node6",
      "id": "talk_p_proc"
    },
    {
      "text": "What is it now? You're really starting to piss me off.",
      "node_id": "node11",
      "id": "Node003"
    },
    {
      "text": "Which \"friend\" are you selling?",
      "node_id": "node34",
      "id": "Node021"
    },
    {
      "text": "",
      "node_id": "node74",
      "id": "Node961"
    },
    {
      "text": "Anything else?",
      "node_id": "node45",
      "id": "Node020"
    },
    {
